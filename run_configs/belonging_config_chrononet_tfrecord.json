{
    "id": "belonging_prediction_chrononet_tfrecord_v1",
    "log_filename": "belonging_results.csv",
    "dataset_params": {
        "name": "load_belonging_tfrecords",
        "tfrecords_dir": "/content/tfrecords/quick_scalograms_tf/1",
        "channels": ["TP9", "AF7", "AF8", "TP10"]
    },
    "preprocessor_params": {
        "name": "tfrecord_preprocessor",
        "test_split": 0.0,
        "val_split": 0.0,
        "batch_size": 1,
        "cv_folds": 3,
        "downsample_train": false,
        "max_windows_per_person": 1000
    },
    "model_params": {
        "name": "ChronoNet",
        "conv_filters": 16,
        "conv_kernel_sizes": [2, 4, 8],
        "conv_stride": 2,
        "num_blocks": 3,
        "gru_hidden_size": 16,
        "dropout_rate": 0.3,
        "use_batch_norm": true,
        "window_batch_size": 8,
        "truncate_bptt": 64
    },
    "trainer_params": {
        "name": "BelongingTrainer",
        "num_epochs": 20,
        "learning_rate": 0.001,
        "weight_decay": 0.0001,
        "patience": 5,
        "threshold_strategy": "max_f1_class0",
        "threshold_bins": 101
    },
    "grid_search": {
        "metric": "accuracy",
        "params": {
            "trainer_params.learning_rate": [0.001, 0.0003],
            "model_params.dropout_rate": [0.2, 0.4],
            "model_params.gru_hidden_size": [16, 32]
        }
    }
}
