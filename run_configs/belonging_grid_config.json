{
    "id": "belonging_prediction_grid_search",
    "grid_keys": ["trainer_params.learning_rate", "model_params.dropout_rate", "preprocessor_params.batch_size"],
    "log_filename": "belonging_grid_search_results.csv",
    "dataset_params": {
        "name": "load_belonging_spectrograms",
        "spectrograms_dir": "scalogram-data/64x64",
        "labels_csv": "GT1_labels.csv",
        "channels": ["TP9", "AF7", "AF8", "TP10"]
    },
    "preprocessor_params": {
        "name": "spectrogram_preprocessor",
        "test_split": 0.2,
        "val_split": 0.2,
        "batch_size": [1],
        "force_single_person_batch": true,
        "split_by_person": true,
        "downsample_train": true,
        "max_windows_per_person": 1000
    },
    "model_params": {
        "name": "BelongingCNN",
        "dropout_rate": [0.2, 0.3, 0.5],
        "conv1_filters": 32,
        "conv2_filters": 64,
        "fc_hidden_size": 128,
        "window_chunk_size": 256
    },
    "trainer_params": {
        "name": "BelongingTrainer",
        "num_epochs": 100,
        "learning_rate": [0.0001, 0.001, 0.01],
        "weight_decay": 0.0001,
        "patience": 15
    }
}
